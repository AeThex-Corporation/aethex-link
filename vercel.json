{
  "buildCommand": "npm run build",
  "framework": "react-router",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "X-Forwarded-Host",
          "value": "$host"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
